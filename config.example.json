{
  "title": "Live Tracker",
  "trackFile": "tracks/start.gpx",
  "translationFile": "translations/en.json",
  "traccarUrl": "https://your-traccar.example.com",
  "token": "REPLACE_WITH_API_TOKEN",
  "refreshSeconds": 8,
  "deviceIds": [1, 2, 3],
  "showViewerLocation": true,
  "staleMinutes": 15,
  "startTime": null,
  "debugStartTime": null,
  "expectedAvgSpeedKph": 60,
  "historyHours": 24,
  "participantMapFile": "participants.json",
  "pollSeconds": 30,
  "offrouteThresholdMeters": 100,
  "offrouteMessage": "{name} is off-route at {time}. Last location: {lat},{lng}",
  "smsGateway": {
    "baseUrl": "https://www.traccar.org/sms/",
    "authorization": "REPLACE_WITH_SMS_GATEWAY_TOKEN",
    "method": "POST",
    "path": "/",
    "headers": {
      "Content-Type": "application/json",
      "Authorization": "{authorization}"
    },
    "bodyFormat": "json",
    "body": {
      "to": "{phone}",
      "message": "{message}"
    }
  }
}
